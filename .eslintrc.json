{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":0,
        "no-unused-vars": ["error", { "vars": "local", "args": "none", "ignoreRestSiblings": true }]
    }
}
